'use strict';

/**
 * @ngdoc directive
 * @name ryosanApp.directive:messageDialog
 * @description
 * # messageDialog
 */
angular.module('ryosanApp')
  .directive('messageDialog', function () {
    return {
      template: '<div></div>',
      restrict: 'E',
      link: function postLink(scope, element, attrs) {
        element.text('this is the messageDialog directive');
      }
    };
  });
